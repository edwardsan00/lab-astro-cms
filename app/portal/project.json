{
  "name": "portal",
  "projectType": "application",
  "sourceRoot": "app/portal/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd app/portal && astro build",
        "outputPath": "app/portal/dist"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd app/portal && astro dev"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd app/portal && astro preview"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "ui"
  ]
}